{
  "name": "sponsor-explorer",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "utils/configs/**",
    "utils/types/**"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky || true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{tsx,ts,js,md,mdx}": [
      "prettier --write .",
      "eslint . --quiet --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash-es": "^4.17.12",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^11.0.3",
    "jotai": "^2.4.3",
    "lodash-es": "^4.17.21",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@kuro-utils/eslint-config-base": "*",
    "@kuro-utils/types": "*",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "clsx": "^2.1.0",
    "cssnano": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
